<!DOCTYPE HTML>
<html>
<head>
	@await Html.PartialAsync("MetatagsPartial")
	@await Html.PartialAsync("CssPartial")
</head>
<body class="left-sidebar is-preload">
	<div id="page-wrapper">

		@await Html.PartialAsync("HeaderPartial")

		<!-- Main Wrapper -->
		<div id="main-wrapper">
			<div class="wrapper style2">
				<div class="inner">
					<div class="container">
						<div class="row">
							@RenderBody()
						</div>
					</div>
				</div>
			</div>
			<div class="wrapper style3">
				@await Html.PartialAsync("PostGalleryPartial");
			</div>
		</div>

		@await Html.PartialAsync("FooterPartial")
	</div>

	@await Html.PartialAsync("ScriptsPartial")

</body>
</html>